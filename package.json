{
    "name": "split-mui-imports",
    "displayName": "split-mui-imports",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.90.0"
    },
    "icon": "images/split-material-ui.png",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onWillSaveTextDocument"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.splitMUIImports",
                "title": "Split MUI Imports"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Split MUI Imports",
            "properties": {
                "splitMUIImports.runOnSave": {
                    "type": "boolean",
                    "default": true,
                    "description": "Run the Split MUI Imports command on save"
                }
            }
        },
        "keybindings": [
            {
                "command": "extension.splitMUIImports",
                "key": "ctrl+win+alt+s",
                "when": "editorTextFocus && !isMac"
            },
            {
                "command": "extension.splitMUIImports",
                "key": "ctrl+cmd+alt+s",
                "when": "editorTextFocus && isMac"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.90.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.4.0"
    }
}
