{
    "name": "split-mui-imports",
    "displayName": "Split MUI Imports",
    "description": "An extension which splits grouped Material-UI imports",
    "publisher": "DarkPhoenix",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.90.0"
    },
    "icon": "images/split-material-ui.png",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onWillSaveTextDocument"
    ],
    "main": "./extension.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/0DarkPhoenix/split-mui-imports.git"
    },
    "contributes": {
        "commands": [
            {
                "command": "extension.splitMUIImports",
                "title": "Split MUI Imports: split MUI imports"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Split MUI Imports",
            "properties": {
                "splitMUIImports.runOnSave": {
                    "type": "boolean",
                    "default": true,
                    "description": "Run the Split MUI Imports command on save"
                },
                "splitMUIImports.showInfoMessages": {
                    "type": "boolean",
                    "default": false,
                    "description": "Show information messages"
                }
            }
        },
        "keybindings": [
            {
                "command": "extension.splitMUIImports",
                "key": "ctrl+win+alt+s",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.90.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.4.0"
    }
}
